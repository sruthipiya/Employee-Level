<odoo>
    <record id="employee_level_id" model="ir.actions.act_window">
        <field name="name">Employee Level</field>
        <field name="res_model">employee.level</field>
        <field name="view_mode">tree</field>
    </record>
    <record id="employee_level_tree_id" model="ir.ui.view">
        <field name="name">Employee Level</field>
        <field name="model">employee.level</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="level"/>
                <field name="salary"/>
            </tree>
        </field>
    </record>
</odoo>